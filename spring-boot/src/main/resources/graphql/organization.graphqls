type Organization {
    id: ID!
    name: String!
    description: String
    address: Address
    phone: String
    email: String
    website: String
    affiliations: [Affiliation]
    type: Type!
    region: String!
    sectors: [Sector]
    approved: Boolean!
    contacts: [Contact]
}

extend type Query {
    getOrganizations: [Organization]
    getOrganization(id: ID!): Organization
}
