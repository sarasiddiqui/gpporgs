type Affiliation {
    id: ID!
    name: String!
}

type Country {
    code: ID!
    name: String!
}

type Language {
    code: String!
    name: String!
}

type Region {
    id: ID!
    name: String!
}

type Sector {
    id: ID!
    name: String!
}

type Type {
    id: ID!
    name: String!
}

extend type Query {
    getAffiliations: [Affiliation]
    getCountries: [Country]
    getLanguages: [Language]
    getRegions: [Region]
    getSectors: [Sector]
    getTypes: [Type]
}
