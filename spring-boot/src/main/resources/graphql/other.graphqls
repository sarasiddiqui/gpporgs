type Affiliation {
    id: ID!
    name: String!
}

input AffiliationInput {
    id: ID!
}

type Country {
    code: ID!
    name: String!
}

input CountryInput {
    code: String!
}

type Language {
    code: String!
    name: String!
}

input LanguageInput {
    code: String!
}

type Region {
    id: ID!
    name: String!
}

type Sector {
    id: ID!
    name: String!
}

input SectorInput {
    id: ID!
}

type Type {
    id: ID!
    name: String!
}

input TypeInput {
    id: ID!
}

enum Area {
    DOMESTIC
    INTERNATIONAL
    ALL
}

extend type Query {
    getAffiliations: [Affiliation]
    getCountries: [Country]
    getLanguages: [Language]
    getRegions: [Region]
    getSectors: [Sector]
    getTypes: [Type]
}
