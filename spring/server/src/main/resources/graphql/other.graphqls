type Affiliation {
    id: ID!
    value: String!
}

type Country {
    code: ID!
    value: String!
}

type Language {
    code: ID!
    value: String!
}

type Region {
    id: ID!
    value: String!
}

type Sector {
    id: ID!
    value: String!
}

type Type {
    id: ID!
    value: String!
}

enum Area {
    ALL, DOMESTIC, INTERNATIONAL
}

extend type Query {
    affiliations: [Affiliation]
    countries: [Country]
    languages: [Language]
    regions: [Region]
    sectors: [Sector]
    types: [Type]
}
